[include]
    path = ~/.config/dotfiles/local/git/local_config

[user]
    email = 49900846+keke1008@users.noreply.github.com
    name = keke1008

[init]
    defaultBranch = main

[alias]
    a = add
    aa = add -A
    au = add -u

    b = branch
    bd = branch -d
    bD = branch -D

    bl = blame

    c = commit
    ca = commit --amend
    cm = commit -m
    cam = commit --amend -m
    cane = commit --amend --no-edit

    cl = clone

    d = diff

    f = fetch

    i = init

    l = log

    m = merge
    ma = merge --abort

    p = pull
    pp = pull --prune

    ps = push
    psf = push --force-with-lease --force-if-includes
    psuo = push -u origin @

    r = reset
    rs = reset --soft
    rh = reset --hard

    rr = restore
    rrs = restore --staged

    rb = rebase
    rbi = rebase -i
    rba = rebase --abort
    rbc = rebase --continue

    s = status
    su = status -u

    si = switch
    sic = switch -c
    sid = switch --detach

    sh = stash
    shu = stash -u
    shp = stash pop

[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[pull]
	rebase = false
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
