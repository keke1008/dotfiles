    pp = pull --prune

    ps = push
    psf = push --force-with-lease --force-if-includes

    r = reset
    rs = reset --soft
    rh = reset --hard

    rr = restore

    rb = rebase
    rbi = rebase -i
    rba = rebase --abort
    rbc = rebase --continue

    s = status
    su = status -u

    si = switch
    sic = switch -c

    sh = stash
    shu = stash -u
    shp = stash pop

